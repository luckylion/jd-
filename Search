function shaixuan()
{
    $("#J_goodsList li.gl-item").each(function(){
        a=$(this);
        a.hide();
        var obj=$(this).find(".gl-i-wrap .p-img div").attr("data-venid");
        //alert(obj);
       	if(obj > 1000000000) a.show();
      });
}
shaixuan();
$(window).scroll(function () {
    shaixuan();
})
